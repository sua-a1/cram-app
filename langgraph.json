{
  "name": "cram-support-agent",
  "version": "0.1.0",
  "description": "Customer support agent for Cram ticketing system",
  "entry": "agents/workflows/ticket-processor-functional.ts",
  "environment": {
    "required": [
      "OPENAI_API_KEY",
      "OPENAI_MODEL",
      "OPENAI_TEMPERATURE",
      "LANGGRAPH_API_KEY",
      "LANGGRAPH_PROJECT",
      "NEXT_PUBLIC_SUPABASE_URL",
      "SUPABASE_SERVICE_ROLE_KEY"
    ],
    "optional": [
      "LANGSMITH_API_KEY",
      "LANGSMITH_PROJECT",
      "LANGSMITH_ENDPOINT",
      "LANGSMITH_TRACING",
      "LANGGRAPH_ENDPOINT",
      "LANGGRAPH_ENVIRONMENT"
    ]
  },
  "build": {
    "command": "npm install --no-package-lock --legacy-peer-deps"
  },
  "node_version": "20",
  "dockerfile_lines": [
    "COPY package*.json ./",
    "RUN npm config set legacy-peer-deps true",
    "RUN npm cache clean --force"
  ],
  "dependencies": [".", "package.json", "package-lock.json"],
  "graphs": {
    "ticket-processor": "./agents/workflows/ticket-processor-functional.ts:agent"
  }
} 