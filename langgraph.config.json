{
  "project": "cram-app",
  "version": "1.0.0",
  "metadata": {
    "description": "Cram App LangGraph API",
    "repository": "https://github.com/sua-a1/cram-app"
  },
  "runtime": {
    "type": "node",
    "version": "18.x"
  },
  "dependencies": [
    "@langchain/core@0.1.0",
    "@langchain/openai@0.0.12",
    "@langchain/langgraph@0.0.8",
    "zod@3.22.4"
  ],
  "graphs": {
    "hello": {
      "entrypoint": "./agents/workflows/hello-world.ts",
      "function": "run",
      "config": {
        "model": "gpt-4-1106-preview",
        "temperature": 0.7
      }
    },
    "process_ticket": {
      "entrypoint": "./agents/workflows/ticket-processor.ts",
      "function": "run",
      "config": {
        "model": "gpt-4-1106-preview",
        "temperature": 0.3
      }
    }
  },
  "environment": "development"
} 